{
  "name": "@catalysisdev/check",
  "version": "0.7.10",
  "description": "A set of utilities responsible checking Catalysis projects",
  "type": "module",
  "exports": {
    "./*": {
      "node": "./dist/public/*.js",
      "types": "./dist/public/*.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "prepack": "cross-env NODE_ENV=production pnpm nx build",
    "build": "nx build",
    "clean": "nx clean",
    "lint": "nx lint",
    "lint:fix": "nx lint:fix",
    "test": "nx run test",
    "test:watch": "nx test:watch",
    "type-check": "nx type-check",
    "postpublish": "pnpm install"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ]
  },
  "dependencies": {
    "@catalysisdev/core": "0.7.10"
  },
  "oclif": {
    "commands": "./dist/public/node/commands",
    "topics": {
      "check": {
        "description": "Runs checks against against a project (style, types)"
      }
    }
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
