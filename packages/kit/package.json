{
  "name": "@gestaltjs/kit",
  "version": "0.7.10",
  "private": false,
  "description": "Set of Getalt models and utilities to extend the Gestalt framework and projects",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    "./*": {
      "node": "./dist/public/*.js",
      "types": "./dist/public/*.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "prepack": "cross-env NODE_ENV=production pnpm nx build",
    "build": "nx build",
    "clean": "nx clean",
    "lint": "nx lint",
    "lint:fix": "nx lint:fix",
    "test": "nx run test",
    "test:watch": "nx test:watch",
    "type-check": "nx type-check",
    "postpublish": "pnpm install"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ],
    "rules": {
      "import/no-nodejs-modules": [
        "off"
      ]
    }
  },
  "dependencies": {},
  "devDependencies": {
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
