{
  "name": "@gestaltjs/core",
  "version": "0.3.2",
  "private": true,
  "description": "Set of Getalt tools and models that are shared across all the features",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    "./cli": {
      "import": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    },
    "./framework": {
      "import": "./dist/framework/index.js",
      "types": "./dist/framework/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "prepack": "cross-env NODE_ENV=production pnpm run build",
    "build": "shx rm -rf dist && rollup -c",
    "lint": "prettier -c src/** && eslint src/**/*.ts",
    "lint:fix": "prettier -w src/** && eslint src/**/*.ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "tsc": "tsc -b --incremental"
  },
  "eslintConfig": {
    "extends": [
      "../../package.json"
    ]
  },
  "devDependencies": {
    "@types/shelljs": "^0.8.11",
    "find-up": "^6.3.0",
    "shelljs": "^0.8.5"
  },
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || >=16.0.0 || >=17.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
