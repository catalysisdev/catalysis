{
  "name": "@catalysisdev/core",
  "version": "0.8.0",
  "private": false,
  "description": "Set of Getalt tools and models that are shared across all the features",
  "keywords": [
    "catalysis",
    "catalysisdev",
    "web"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    "./*": {
      "node": "./dist/public/*",
      "types": "./dist/public/*.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "prepack": "cross-env NODE_ENV=production pnpm nx build",
    "build": "nx build",
    "clean": "nx clean",
    "lint": "nx lint",
    "lint:fix": "nx lint:fix",
    "test": "nx run test",
    "test:watch": "nx test:watch",
    "type-check": "nx type-check",
    "postpublish": "pnpm install"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ],
    "rules": {
      "import/no-nodejs-modules": [
        "off"
      ]
    }
  },
  "dependencies": {
    "@oclif/core": "1.26.1",
    "@remix-run/web-fetch": "^4.2.0",
    "acorn": "8.8.2",
    "case-anything": "2.1.10",
    "commondir": "1.0.1",
    "esbuild": "0.17.7",
    "execa": "6.1.0",
    "fast-glob": "3.2.12",
    "find-up": "6.3.0",
    "fs-extra": "11.1.0",
    "handlebars": "4.7.7",
    "inquirer": "9.1.4",
    "listr2": "5.0.7",
    "magic-string": "0.27.0",
    "noop-stream": "1.0.0",
    "ohmyfetch": "0.4.21",
    "parse-json": "6.0.2",
    "path-equal": "1.2.5",
    "pathe": "1.1.0",
    "picocolors": "1.0.0",
    "plur": "5.1.0",
    "radix3": "1.0.0",
    "safe-stable-stringify": "^2.4.2",
    "source-map-support": "0.5.21",
    "stacktracey": "2.1.8",
    "strip-ansi": "7.0.1",
    "tempy": "3.0.0",
    "terminal-link": "3.0.0",
    "typed-file-system-path": "0.12.0",
    "unplugin": "1.0.1",
    "username": "6.0.0",
    "vite": "4.1.1",
    "yargs": "17.6.2",
    "zod": "3.20.6"
  },
  "devDependencies": {
    "@types/inquirer": "9.0.3",
    "@types/yargs": "17.0.22",
    "rimraf": "4.1.2"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
