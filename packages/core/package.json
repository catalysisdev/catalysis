{
  "name": "@gestaltjs/core",
  "version": "0.7.8",
  "private": false,
  "description": "Set of Getalt tools and models that are shared across all the features",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    "./node/path": {
      "node": "./dist/node/path.js",
      "types": "./dist/node/path.d.ts"
    },
    "./node/fs": {
      "node": "./dist/node/fs.js",
      "types": "./dist/node/fs.d.ts"
    },
    "./node/command": {
      "node": "./dist/node/command.js",
      "types": "./dist/node/command.d.ts"
    },
    "./node/tsc": {
      "node": "./dist/node/tsc.js",
      "types": "./dist/node/tsc.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    },
    "./runtime": {
      "import": "./dist/runtime/index.js",
      "types": "./dist/runtime/index.d.ts"
    },
    "./shared": {
      "import": "./dist/shared/index.js",
      "types": "./dist/shared/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "prepack": "cross-env NODE_ENV=production pnpm run build",
    "build": "pnpm clean && rollup -c ./rollup.config.js",
    "lint": "prettier -c src/** && eslint './**/*.{js,ts}'",
    "lint:fix": "prettier -w src/** && eslint './**/*.{js,ts}' --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "tsc": "tsc && echo"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ],
    "rules": {
      "import/no-nodejs-modules": [
        "off"
      ]
    }
  },
  "peerDependencies": {
    "electron": "^18.2.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "electron": {
      "optional": true
    }
  },
  "dependencies": {
    "@oclif/core": "1.7.0",
    "acorn": "8.7.1",
    "case-anything": "2.1.10",
    "commondir": "1.0.1",
    "electron": "19.0.4",
    "enquirer": "2.3.6",
    "fast-glob": "3.2.11",
    "fast-json-stringify": "4.1.0",
    "fastify": "4.0.1",
    "find-up": "6.3.0",
    "fs-extra": "10.1.0",
    "handlebars": "4.7.7",
    "listr2": "4.0.5",
    "magic-string": "0.26.2",
    "noop-stream": "1.0.0",
    "path-equal": "1.2.1",
    "pathe": "0.3.0",
    "picocolors": "1.0.0",
    "pino": "8.0.0",
    "pino-abstract-transport": "0.5.0",
    "pino-pretty": "8.0.0",
    "plur": "5.1.0",
    "radix3": "0.1.2",
    "rehype-parse": "8.0.4",
    "rehype-stringify": "9.0.3",
    "source-map-support": "0.5.21",
    "stacktracey": "2.1.8",
    "tempy": "2.0.0",
    "terminal-link": "3.0.0",
    "unified": "10.1.2",
    "vite": "2.9.12"
  },
  "devDependencies": {
    "@types/estree": "0.0.51",
    "@types/pino": "7.0.5"
  },
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || >=16.0.0 || >=17.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
