{
  "name": "@gestaltjs/core",
  "version": "0.7.4",
  "private": false,
  "description": "Set of Getalt tools and models that are shared across all the features",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    "./cli": {
      "import": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    },
    "./runtime": {
      "import": "./dist/runtime/index.js",
      "types": "./dist/runtime/index.d.ts"
    },
    "./shared": {
      "import": "./dist/shared/index.js",
      "types": "./dist/shared/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "prepack": "cross-env NODE_ENV=production pnpm run build",
    "build": "pnpm clean && rollup -c",
    "lint": "prettier -c src/** && eslint 'src/**/*.ts'",
    "lint:fix": "prettier -w src/** && eslint 'src/**/*.ts' --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --reporter=default --reporter=junit --outputFile=./junit-core.xml",
    "tsc": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ],
    "rules": {
      "import/no-nodejs-modules": [
        "off"
      ]
    }
  },
  "dependencies": {
    "@oclif/core": "^1.7.0",
    "pino": "^7.8.0",
    "pino-abstract-transport": "^0.5.0",
    "pino-pretty": "^7.5.1",
    "source-map-support": "^0.5.21",
    "stacktracey": "^2.1.8",
    "vite": "2.9.6"
  },
  "devDependencies": {
    "@types/pino": "7.0.5",
    "@types/shelljs": "0.8.11",
    "case-anything": "2.1.10",
    "commondir": "^1.0.1",
    "fast-glob": "^3.2.11",
    "fastify": "3.29.0",
    "find-up": "6.3.0",
    "fs-extra": "10.1.0",
    "handlebars": "4.7.7",
    "noop-stream": "1.0.0",
    "path-equal": "^1.1.3",
    "pathe": "0.2.0",
    "picocolors": "1.0.0",
    "radix3": "^0.1.1",
    "shelljs": "0.8.5",
    "tempy": "2.0.0",
    "terminal-link": "3.0.0"
  },
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || >=16.0.0 || >=17.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
