{
  "name": "@gestaltjs/core",
  "version": "0.7.10",
  "private": false,
  "description": "Set of Getalt tools and models that are shared across all the features",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    "./common/*": {
      "node": "./dist/common/*.js",
      "types": "./dist/common/*.d.ts"
    },
    "./node/*": {
      "node": "./dist/node/*.js",
      "types": "./dist/node/*.d.ts"
    },
    "./testing/*": {
      "node": "./dist/testing/*.js",
      "types": "./dist/testing/*.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "prepack": "cross-env NODE_ENV=production pnpm nx build",
    "build": "nx build",
    "clean": "nx clean",
    "lint": "nx lint",
    "lint:fix": "nx lint:fix",
    "test": "nx run test",
    "test:watch": "nx test:watch",
    "type-check": "nx type-check",
    "postpublish": "pnpm install"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ],
    "rules": {
      "import/no-nodejs-modules": [
        "off"
      ]
    }
  },
  "dependencies": {
    "@oclif/core": "1.16.0",
    "acorn": "8.8.0",
    "case-anything": "2.1.10",
    "commondir": "1.0.1",
    "electron": "20.1.1",
    "fast-glob": "3.2.11",
    "fast-json-stringify": "5.2.0",
    "fastify": "4.5.3",
    "find-up": "6.3.0",
    "fs-extra": "10.1.0",
    "handlebars": "4.7.7",
    "inquirer": "9.1.1",
    "listr2": "5.0.2",
    "magic-string": "0.26.3",
    "noop-stream": "1.0.0",
    "path-equal": "1.2.2",
    "pathe": "0.3.5",
    "picocolors": "1.0.0",
    "pino": "8.4.2",
    "pino-abstract-transport": "1.0.0",
    "pino-pretty": "9.1.0",
    "plur": "5.1.0",
    "radix3": "0.1.2",
    "rehype-parse": "8.0.4",
    "rehype-stringify": "9.0.3",
    "source-map-support": "0.5.21",
    "stacktracey": "2.1.8",
    "tempy": "2.0.0",
    "terminal-link": "3.0.0",
    "ts-error": "1.0.6",
    "unified": "10.1.2",
    "vite": "3.0.9"
  },
  "devDependencies": {
    "@types/inquirer": "9.0.1"
  },
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || >=16.0.0 || >=18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
