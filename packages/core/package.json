{
  "name": "@gestaltjs/core",
  "version": "0.7.10",
  "private": false,
  "description": "Set of Getalt tools and models that are shared across all the features",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    "./*": {
      "node": "./dist/public/*.js",
      "types": "./dist/public/*.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "prepack": "cross-env NODE_ENV=production pnpm nx build",
    "build": "nx build",
    "clean": "nx clean",
    "lint": "nx lint",
    "lint:fix": "nx lint:fix",
    "test": "nx run test",
    "test:watch": "nx test:watch",
    "type-check": "nx type-check",
    "postpublish": "pnpm install"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ],
    "rules": {
      "import/no-nodejs-modules": [
        "off"
      ]
    }
  },
  "dependencies": {
    "@oclif/core": "1.20.4",
    "@remix-run/web-fetch": "^4.2.0",
    "acorn": "8.8.1",
    "case-anything": "2.1.10",
    "commondir": "1.0.1",
    "esbuild": "0.15.15",
    "execa": "6.1.0",
    "fast-glob": "3.2.12",
    "fast-safe-stringify": "2.1.1",
    "find-up": "6.3.0",
    "fs-extra": "10.1.0",
    "handlebars": "4.7.7",
    "inquirer": "9.1.4",
    "listr2": "5.0.5",
    "magic-string": "0.26.7",
    "noop-stream": "1.0.0",
    "ohmyfetch": "0.4.21",
    "parse-json": "6.0.2",
    "path-equal": "1.2.4",
    "pathe": "1.0.0",
    "picocolors": "1.0.0",
    "pino": "8.7.0",
    "pino-abstract-transport": "1.0.0",
    "pino-pretty": "9.1.1",
    "plur": "5.1.0",
    "radix3": "1.0.0",
    "source-map-support": "0.5.21",
    "stacktracey": "2.1.8",
    "strip-ansi": "7.0.1",
    "tempy": "3.0.0",
    "terminal-link": "3.0.0",
    "ts-error": "1.0.6",
    "typed-file-system-path": "0.11.0",
    "unplugin": "1.0.0",
    "username": "6.0.0",
    "vite": "3.2.4"
  },
  "devDependencies": {
    "@types/inquirer": "9.0.3"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
