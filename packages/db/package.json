{
  "name": "@gestaltjs/db",
  "version": "0.7.10",
  "description": "A set of utilities for managing and interacting with databases",
  "type": "module",
  "files": [
    "/dist"
  ],
  "scripts": {
    "prepack": "cross-env NODE_ENV=production pnpm nx build",
    "build": "nx build",
    "clean": "nx clean",
    "lint": "nx lint",
    "lint:fix": "nx lint:fix",
    "test": "nx run test",
    "test:watch": "nx test:watch",
    "type-check": "nx type-check",
    "postpublish": "pnpm install"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ]
  },
  "dependencies": {
    "@gestaltjs/core": "0.7.10",
    "@oclif/core": "1.16.3"
  },
  "oclif": {
    "commands": "./dist/node/commands",
    "topics": {
      "db": {
        "description": "Create and manage databases"
      }
    }
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
