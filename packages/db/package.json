{
  "name": "@gestaltjs/db",
  "version": "0.3.2",
  "private": true,
  "description": "A set of utilities for managing and interacting with databases",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "/src",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "prepublishOnly": "pnpm run build",
    "build": "shx rm -rf dist && tsc -b",
    "build:watch": "tsc -b --watch",
    "lint": "eslint src/**",
    "lint:fix": "eslint src/** --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "tsc": "tsc -b --incremental"
  },
  "eslintConfig": {
    "extends": [
      "../../package.json"
    ]
  },
  "devDependencies": {
    "@gestaltjs/core": "0.3.2",
    "@gestaltjs/support": "0.3.2"
  },
  "oclif": {
    "commands": "./dist/commands",
    "plugins": []
  }
}
