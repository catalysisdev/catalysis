{
  "name": "@gestaltjs/eslint-plugin",
  "version": "0.7.10",
  "private": false,
  "description": "A plugin that provides an ESLint configuration for projects to extend from and custom Gestalt-specific rules",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "eslint"
  ],
  "main": "dist/index.js",
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "scripts": {
    "prepack": "cross-env NODE_ENV=production pnpm nx build",
    "build": "nx build",
    "clean": "nx clean",
    "lint": "nx lint",
    "lint:fix": "nx lint:fix",
    "test": "nx run test",
    "test:watch": "nx test:watch",
    "type-check": "nx type-check",
    "postpublish": "pnpm install"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ]
  },
  "dependencies": {
    "eslint": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "typescript": "4.8.3"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
