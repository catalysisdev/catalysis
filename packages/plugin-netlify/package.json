{
  "name": "@gestaltjs/gestalt-plugin-netlify",
  "version": "0.7.8",
  "private": false,
  "description": "Gestalt's plugin to adapt the project to deploy it to Netlify",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web",
    "netlify"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/gestalt.plugin.js",
      "types": "./dist/gestalt.plugin.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "prepack": "cross-env NODE_ENV=production pnpm run build",
    "build": "pnpm clean && rollup -c ./rollup.config.js",
    "lint": "prettier -c src/** && eslint './**/*.{js,ts}'",
    "lint:fix": "prettier -w src/** && eslint './**/*.{js,ts}' --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "tsc": "tsc && echo"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ]
  },
  "dependencies": {
    "@gestaltjs/plugins": "0.7.8"
  },
  "devDependencies": {
    "@gestaltjs/plugins": "0.7.8"
  },
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || >=16.0.0 || >=17.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
