{
  "name": "gestaltjs",
  "version": "0.7.10",
  "private": false,
  "description": "A tool for building Gestalt web app",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "type": "module",
  "bin": {
    "gestalt": "./bin/run.js"
  },
  "files": [
    "/bin/run.js",
    "/bin/run.cmd",
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./configuration": {
      "import": "./dist/node/manifests/configuration.js",
      "types": "./dist/node/manifests/configuration.d.ts"
    },
    "./target": {
      "import": "./dist/node/manifests/target.js",
      "types": "./dist/node/manifests/target.d.ts"
    },
    "./plugin": {
      "import": "./dist/node/manifests/plugin.js",
      "types": "./dist/node/manifests/plugin.d.ts"
    }
  },
  "scripts": {
    "prepack": "cross-env NODE_ENV=production pnpm nx build",
    "build": "nx build",
    "clean": "nx clean",
    "lint": "nx lint",
    "lint:fix": "nx lint:fix",
    "test": "nx run test",
    "test:watch": "nx test:watch",
    "type-check": "nx type-check",
    "postpublish": "pnpm install"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ]
  },
  "dependencies": {
    "@gestaltjs/build": "0.7.10",
    "@gestaltjs/check": "0.7.10",
    "@gestaltjs/db": "0.7.10",
    "@gestaltjs/generate": "0.7.10",
    "@gestaltjs/dev": "0.7.10",
    "@gestaltjs/test": "0.7.10",
    "@gestaltjs/info": "0.7.10",
    "@gestaltjs/routes": "0.7.10",
    "@gestaltjs/core": "0.7.10",
    "@oclif/core": "1.16.0",
    "@oclif/plugin-help": "5.1.12"
  },
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || >=16.0.0 || >=18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "oclif": {
    "bin": "gestalt",
    "topicSeparator": " ",
    "plugins": [
      "@gestaltjs/build",
      "@gestaltjs/check",
      "@gestaltjs/db",
      "@gestaltjs/dev",
      "@gestaltjs/test",
      "@gestaltjs/info",
      "@gestaltjs/routes",
      "@gestaltjs/generate",
      "@oclif/plugin-help"
    ]
  }
}
