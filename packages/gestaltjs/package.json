{
  "name": "gestaltjs",
  "version": "0.7.8",
  "private": false,
  "description": "A tool for building Gestalt web app",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "type": "module",
  "bin": {
    "gestalt": "./bin/run.js"
  },
  "files": [
    "/bin/run.js",
    "/bin/run.cmd",
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./support": {
      "import": "./dist/runtime/support.js",
      "types": "./dist/runtime/support.d.ts"
    },
    "./configuration": {
      "import": "./dist/runtime/configuration.js",
      "types": "./dist/runtime/configuration.d.ts"
    },
    "./target": {
      "import": "./dist/runtime/target.js",
      "types": "./dist/runtime/target.d.ts"
    },
    "./db": {
      "import": "./dist/runtime/db.js",
      "types": "./dist/runtime/db.d.ts"
    },
    "./plugin": {
      "import": "./dist/runtime/plugin.js",
      "types": "./dist/runtime/plugin.d.ts"
    }
  },
  "scripts": {
    "clean": "shx rm -rf dist",
    "prepack": "cross-env NODE_ENV=production pnpm run build",
    "build": "pnpm clean && rollup -c ./rollup.config.js",
    "lint": "prettier -c src/** && eslint 'src/**/*.ts'",
    "lint:fix": "prettier -w src/** && eslint 'src/**/*.ts' --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "tsc": "tsc && echo"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ]
  },
  "dependencies": {
    "@gestaltjs/build": "0.7.8",
    "@gestaltjs/check": "0.7.8",
    "@gestaltjs/db": "0.7.8",
    "@gestaltjs/generate": "0.7.8",
    "@gestaltjs/dev": "0.7.8",
    "@gestaltjs/test": "0.7.8",
    "@gestaltjs/info": "0.7.8",
    "@gestaltjs/routes": "0.7.8",
    "@gestaltjs/core": "0.7.8",
    "@gestaltjs/plugins": "0.7.8",
    "@oclif/core": "^1.7.0",
    "@oclif/plugin-help": "^5"
  },
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || >=16.0.0 || >=17.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "oclif": {
    "bin": "gestalt",
    "topicSeparator": " ",
    "plugins": [
      "@gestaltjs/build",
      "@gestaltjs/check",
      "@gestaltjs/db",
      "@gestaltjs/dev",
      "@gestaltjs/test",
      "@gestaltjs/info",
      "@gestaltjs/routes",
      "@gestaltjs/generate",
      "@gestaltjs/plugins",
      "@oclif/plugin-help"
    ]
  },
  "nx": {
    "implicitDependencies": [
      "@gestaltjs/eslint-plugin"
    ]
  }
}
