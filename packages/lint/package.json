{
  "name": "@gestaltjs/lint",
  "version": "0.3.2",
  "private": false,
  "description": "A set of utilities responsible linting Gestalt apps",
  "keywords": [
    "gestalt",
    "gestaltjs"
  ],
  "license": "MIT",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "pnpm run build",
    "build": "shx rm -rf dist && tsc -b",
    "build:watch": "tsc -b --watch",
    "lint": "eslint src/**",
    "lint:fix": "eslint src/** --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "tsc": "tsc -b --incremental"
  },
  "eslintConfig": {
    "extends": [
      "../../package.json"
    ]
  },
  "dependencies": {},
  "peerDependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@gestaltjs/core": "0.3.2",
    "@gestaltjs/support": "0.3.2"
  },
  "devDependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@gestaltjs/core": "0.3.2",
    "@gestaltjs/support": "0.3.2"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 12"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "oclif": {
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
