{
  "name": "@gestaltjs/testing",
  "version": "0.1.0",
  "private": true,
  "description": "Set of internal utilities for testing",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "prepack": "pnpm run build",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "prettier -c src/** && eslint src/**/*.ts",
    "lint:fix": "prettier -w src/** && eslint src/**/*.ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "tsc": "tsc -b --incremental"
  },
  "eslintConfig": {
    "extends": [
      "../../package.json"
    ]
  },
  "devDependencies": {},
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || >=16.0.0 || >=17.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
