{
  "name": "@gestaltjs/testing",
  "version": "0.1.0",
  "private": true,
  "description": "Set of internal utilities for testing",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    "./node/models": {
      "node": "./dist/node/models.js",
      "types": "./dist/node/models.d.ts"
    },
    "./node/temporary": {
      "node": "./dist/node/temporary.js",
      "types": "./dist/node/temporary.d.ts"
    },
    "./node/workspace": {
      "node": "./dist/node/workspace.js",
      "types": "./dist/node/workspace.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "prepack": "pnpm run build",
    "build": "true",
    "lint": "prettier -c src/** && eslint './**/*.{js,ts}'",
    "lint:fix": "prettier -w src/** && eslint './**/*.{js,ts}' --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "tsc": "tsc && echo"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ]
  },
  "devDependencies": {
    "@types/rimraf": "3.0.2",
    "pathe": "0.3.0",
    "rimraf": "3.0.2",
    "tempy": "2.0.0",
    "@gestaltjs/core": "workspace:*"
  },
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || >=16.0.0 || >=17.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
