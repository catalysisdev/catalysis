{
	"name": "@gestaltjs/serve",
	"version": "0.3.2",
	"private": true,
	"description": "A set of utilities responsible for serving the app",
	"main": "src/index.ts",
	"types": "src/index.ts",
	"files": [
		"/src",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"scripts": {
		"clean": "shx rm -rf dist",
		"prepack": "pnpm run build",
		"build": "shx rm -rf dist && tsc -b",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint src/**/*.ts --fix",
		"test": "vitest run",
		"test:watch": "vitest",
		"tsc": "tsc -b --incremental",
		"dev": "node server"
	},
	"eslintConfig": {
		"extends": [
			"../../package.json"
		]
	},
	"peerDependencies": {
		"@gestaltjs/core": "0.3.2",
		"@oclif/core": "0.5.10"
	},
	"oclif": {
		"commands": "./dist/commands",
		"plugins": []
	},
	"devDependencies": {
		"@vitejs/plugin-react": "^1.2.0",
		"express": "^4.17.2",
		"vite": "^2.8.3"
	},
	"dependencies": {
		"find-up": "^6.2.0"
	}
}
