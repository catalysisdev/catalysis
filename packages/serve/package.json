{
  "name": "@gestaltjs/serve",
  "version": "0.3.2",
  "private": true,
  "description": "A set of utilities responsible for serving the app",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "/src",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "prepack": "pnpm run build",
    "build": "true",
    "lint": "prettier -c src/** && eslint src/**/*.ts",
    "lint:fix": "prettier -w src/** && eslint src/**/*.ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "tsc": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "../../package.json"
    ]
  },
  "devDependencies": {
    "@gestaltjs/core": "0.3.2",
    "@gestaltjs/testing": "0.1.0",
    "@oclif/core": "0.5.10"
  },
  "oclif": {
    "commands": "./dist/commands",
    "plugins": []
  },
  "dependencies": {
    "@types/react-dom": "^17.0.11",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1"
  }
}
