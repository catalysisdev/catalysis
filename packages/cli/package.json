{
  "name": "@gestaltjs/cli",
  "version": "0.2.0",
  "private": false,
  "description": "A CLI tool for building Gestalt web app",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "types": "dist/index.d.ts",
  "bin": {
    "gestalt": "./bin/gestalt-run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "pnpm run build",
    "build": "shx rm -rf dist && tsc -b",
    "build:watch": "tsc -b --watch",
    "lint": "prettier -c src/** && eslint src/**",
    "lint:fix": "prettier src/** && eslint src/** --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "tsc": "tsc -b --incremental"
  },
  "eslintConfig": {
    "extends": [
      "../../package.json"
    ]
  },
  "dependencies": {
    "@gestaltjs/cli-core": "0.2.0",
    "@gestaltjs/cli-support": "0.2.0",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1"
  },
  "devDependencies": {
    "@oclif/test": "^2"
  },
  "engine-strict": true,
  "engines": {
    "node": "^12 || ^14 || ^16"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "oclif": {
    "bin": "gestalt",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  }
}
