{
  "name": "@gestaltjs/gestalt-plugin-vue",
  "version": "0.7.8",
  "private": false,
  "description": "Gestalt's plugin that makes Vue the default rendering engine for Gestalt projects",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/gestalt.plugin.js",
      "types": "./dist/gestalt.plugin.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "prepack": "cross-env NODE_ENV=production pnpm run build",
    "build": "pnpm clean && rollup -c ./rollup.config.js",
    "lint": "prettier -c src/** && eslint 'src/**/*.ts'",
    "lint:fix": "prettier -w src/** && eslint 'src/**/*.ts' --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "tsc": "tsc && echo"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ]
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "@gestaltjs/plugins": "^0.7.8"
  },
  "devDependencies": {
    "@gestaltjs/plugins": "0.7.8",
    "vue": "^3.2.33",
    "vite": "2.9.9"
  },
  "peerDependencies": {
    "vue": "^3.2.33"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": false
    }
  },
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || >=16.0.0 || >=17.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
