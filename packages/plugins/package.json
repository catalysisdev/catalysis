{
  "name": "@gestaltjs/plugins",
  "version": "0.7.5",
  "private": false,
  "description": "Models and convenient utilities for implementing Gestalt plugins",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/runtime/index.js",
      "types": "./dist/runtime/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "prepack": "cross-env NODE_ENV=production pnpm run build",
    "build": "pnpm clean && rollup -c ./rollup.config.js",
    "lint": "prettier -c src/** && eslint 'src/**/*.ts'",
    "lint:fix": "prettier -w src/** && eslint 'src/**/*.ts' --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --reporter=default --reporter=junit --outputFile=./junit-core.xml",
    "tsc": "tsc && echo"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ]
  },
  "peerDependencies": {
    "@gestaltjs/core": "0.7.5",
    "@oclif/core": "1.7.0"
  },
  "devDependencies": {
    "@gestaltjs/core": "0.7.5",
    "@oclif/core": "1.7.0"
  },
  "oclif": {
    "commands": "./dist/cli/commands",
    "topics": {
      "plugins": {
        "description": "Create and build Gestalt plugins"
      }
    }
  },
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || >=16.0.0 || >=17.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
