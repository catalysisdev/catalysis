{
  "name": "@gestaltjs/plugins",
  "version": "0.7.8",
  "private": false,
  "description": "Models and convenient utilities for implementing Gestalt plugins",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "type": "module",
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "prepack": "cross-env NODE_ENV=production pnpm run build",
    "build": "pnpm clean && rollup -c ./rollup.config.js",
    "lint": "prettier -c src/** && eslint './**/*.{js,ts}'",
    "lint:fix": "prettier -w src/** && eslint './**/*.{js,ts}' --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "tsc": "tsc && echo"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ]
  },
  "dependencies": {
    "@gestaltjs/core": "^0.7.7",
    "@oclif/core": "^1.9.5"
  },
  "oclif": {
    "commands": "./dist/node/commands",
    "topics": {
      "plugins": {
        "description": "Create and build Gestalt plugins"
      }
    }
  },
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || >=16.0.0 || >=17.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
