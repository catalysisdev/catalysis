{
  "name": "@gestaltjs/create-project",
  "version": "0.7.3",
  "private": false,
  "description": "Create a new Gestalt app",
  "type": "module",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "bin": {
    "gestalt": "./bin/run.js"
  },
  "files": [
    "/bin/run.js",
    "/bin/run.cmd",
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "shx rm -rf dist",
    "prepack": "cross-env NODE_ENV=production pnpm run build",
    "build": "pnpm clean && rollup -c",
    "lint": "prettier -c src/** && eslint 'src/**/*.ts'",
    "lint:fix": "prettier -w src/** && eslint 'src/**/*.ts' --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --reporter=default --reporter=junit --outputFile=./junit-create-project.xml",
    "tsc": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ]
  },
  "dependencies": {
    "@oclif/core": "1.7.0"
  },
  "devDependencies": {},
  "oclif": {
    "bin": "create-project",
    "commands": "./dist/commands"
  },
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || >=16.0.0 || >=17.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
