{
  "name": "@gestaltjs/create-plugin",
  "version": "0.7.10",
  "private": false,
  "description": "Create a new Gestalt plugin",
  "type": "module",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "bin": {
    "gestalt": "./bin/run.js"
  },
  "files": [
    "/bin/run.js",
    "/bin/run.cmd",
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "cross-env NODE_ENV=production pnpm nx build",
    "build": "nx build",
    "clean": "nx clean",
    "lint": "nx lint",
    "lint:fix": "nx lint:fix",
    "test": "nx run test",
    "test:watch": "nx test:watch",
    "type-check": "nx type-check",
    "postpublish": "pnpm install"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ]
  },
  "dependencies": {
    "@oclif/core": "1.19.0",
    "@gestaltjs/core": "0.7.10"
  },
  "oclif": {
    "bin": "create-project",
    "commands": "./dist/public/node/commands"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
